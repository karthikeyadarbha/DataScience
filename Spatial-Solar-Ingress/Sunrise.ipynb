   "source": [
    "from astral.sun import sun\n",
    "from astral import LocationInfo\n",
    "import datetime\n",
    "loc = LocationInfo(name='', region='', timezone='GMT',\n",
    "                   latitude=12.9716, longitude=77.496)\n",
    "print(loc)\n",
    "s = sun(loc.observer, date=datetime.date(2022, 11, 8), tzinfo=loc.timezone)\n",
    "print(s)"
   ]
